{
    "name": "liip/translation-bundle",
    "type": "symfony-bundle",
    "description": "provide various tools to ease translation management",
    "keywords": ["translation", "i18n"],
    "license": "MIT",
    "authors": [
        {
            "name": "Liip AG",
            "homepage": "http://www.liip.ch/"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/liip/LiipTranslationBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.3.9|^7.0",
        "symfony/framework-bundle": "~2.3|~3.0"
    },
    "require-dev": {
        "twig/twig": "~1.0",
        "liip/rmt": "~1.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/yaml": "~2.3|~3.0",
        "symfony/finder": "~2.3|~3.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/validator": "~2.3|~3.0",
        "symfony/dom-crawler": "~2.3|~3.0",
        "symfony/css-selector": "~2.3|~3.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0",
        "symfony/security-bundle": "~2.3|~3.0",
        "liip/functional-test-bundle": "~1.0"
    },
    "suggest": {
        "twig/twig": "~1.0"
    },
    "autoload": {
        "psr-4": { "Liip\\TranslationBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
